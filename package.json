{
  "name": "@jlozovei/trim-currency",
  "version": "0.0.1",
  "description": "A cool JS helper to trim/clean currency values",
  "main": "index.js",
  "scripts": {
    "test:dev": "jest",
    "test": "jest --coverage --watchAll=false",
    "posttest": "yarn jest-coverage-badges && yarn make-coverage-badge"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jlozovei/trim-currency.git"
  },
  "author": "Julio Lozovei <jlozovei@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jlozovei/trim-currency/issues"
  },
  "homepage": "https://github.com/jlozovei/trim-currency#readme",
  "dependencies": {
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.6",
    "@babel/register": "^7.8.6",
    "jest": "^25.1.0",
    "rollup": "^1.32.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "jest-coverage-badges": "^1.1.2",
    "make-coverage-badge": "^1.2.0",
    "prettier": "^1.19.1"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ]
  }
}
